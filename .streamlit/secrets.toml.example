# Streamlit Cloud secrets — copy to .streamlit/secrets.toml for local use
# On Streamlit Cloud: paste these in App Settings → Secrets
# Never commit secrets.toml — it is gitignored

[roamler]
api_key      = "your_roamler_api_key_here"
api_base_url = "https://api.roamler.com"
date_from    = "2025-01-01"
date_to      = "2025-12-31"

[wiser]
api_key      = "your_wiser_api_key_here"
api_base_url = "https://api.wiser.com"

[pinion]
api_key      = "your_pinion_api_key_here"
api_base_url = "https://api.pinion.com"
